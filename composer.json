{
    "name": "sunnysideup/core-modules-dev",
    "description": "Nice modules to include on your dev install.",
    "type": "silverstripe-recipe",
    "keywords": [
        "dev",
        "developer tools"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/extension-installer": "^1.3",
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*",
        "sunnysideup/database-share-clean-up": "dev-master",
        "sunnysideup/dataintegritytests": "dev-master",
        "sunnysideup/delete-members": "dev-master",
        "sunnysideup/vardump": "dev-master",
        "sunnysideup/templateoverview": "dev-master",
        "sunnysideup/typography": "dev-master",
        "oddnoc/silverstripe-artefactcleaner": "*",
        "sunnysideup/cleanup-tables": "dev-master",
        "sunnysideup/gitignore": "dev-main",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\CoreModulesDev\\": "src/"
        }
    },
    "suggests": {
        "silverstripe/standards": "Great, but phpstan/phpstan causes issues!"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ]
}
